<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="br.com.geconp.geconp.HistRetiradoActivity">

    <TableRow android:layout_weight="0">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Data"
            android:id="@+id/txtIdRet"
            android:layout_column="0"
            android:textColor="#ff000000"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Valor"
            android:id="@+id/txtValorRet"
            android:layout_column="0"
            android:layout_marginLeft="20dp"
            android:textColor="#ff000000"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Banco"
            android:id="@+id/txtBancoRet"
            android:layout_column="0"
            android:layout_marginLeft="20dp"
            android:textColor="#ff000000"
            />
    </TableRow>

    <TableRow android:layout_weight="0">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Data"
            android:id="@+id/txtViewHistRetData"
            android:layout_column="0"
            android:textColor="#ffff121e"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Valor"
            android:id="@+id/txtViewHistRetValor"
            android:layout_column="0"
            android:layout_marginLeft="20dp"
            android:textColor="#ffff121e"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Banco"
            android:id="@+id/txtViewHistRetBanco"
            android:layout_column="0"
            android:layout_marginLeft="20dp"
            android:textColor="#ffff121e"
            />

    </TableRow>

</TableLayout>

